{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb",
          "includeFiles": [
            "dist/**",
            "package.json",
            "node_modules/**",
            "rollup.config.js",
            ".env"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ]
  }
  